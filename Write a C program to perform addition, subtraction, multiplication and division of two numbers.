 #include <stdio.h>
 

int main() {
    int n1,n2,n3;
    printf("Input any two numbers separated by comma : ");
    scanf("%d,%d",&n1,&n2);
    printf("The sum of the given numbers  : %d",n1+n2);
    printf("\nThe difference  of the given numbers  : %d",n1-n2);

    printf("\nThe product  of the given numbers  : %d",n1*n2);


    printf("\nThe sum of the given numbers  : %f",(float)n1/(float)n2);

  
   
    
    
     
    
    return 0;
}
